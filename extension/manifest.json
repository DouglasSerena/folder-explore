{
    "manifest_version": 2,
    "name": "folder explorer",
    "author": "Douglas Serena",
    "description": "Navegador de arquivos.",
    "version": "1.0",
    "browser_action": {
        "default_icon": "icon.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["icons.js", "render.js", "main.js"],
            "css": ["main.css"]
        }
    ]
}
